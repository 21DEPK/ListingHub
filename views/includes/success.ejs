<% if(success && success.length){ %>
<div class="row px-4 rounded-3">
  <div
    class="col-md-4 mt-1 offset-md-4 alert alert-success alert-dismissible fade show"
  >
    <%= success %>
    <button
      type="button"
      class="btn-close"
      data-bs-dismiss="alert"
      aria-label="Close"
    ></button>
  </div>
</div>
<% } %>
